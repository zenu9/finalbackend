<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Stock Market</title>
    <link rel="stylesheet" href="charts.css">
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
</head>
<body>
  <nav>
    <ul>
      <li><a href="/">Home</a></li>
      <li><a href="/about">About</a></li>
      <li><a href="/contact">Contact</a></li>
      <li><a href="/items">Items</a></li>
      <li><a href="/stocks">Market</a></li>
      <li><a href="/earnings">IBM</a></li>
      <li><a href="/nasa">Nasa</a></li>
      <% if (username) { %>
        <li style="float:right"><a class="active" href="/logout">Logout</a></li>
        <li style="float:right"><a class="active"><%= username %>!</a></li>
      <% } else { %>
        <li style="float:right"><a class="active" href="/register">Register</a></li>
        <li style="float:right"><a class="active" href="/login">Login</a></li>
      <% } %>
    </ul>
  </nav>
  <h1>Monthly Closing Prices (US States)</h1>
  <div>
    <canvas id="myChart"></canvas>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

  <script>
    const ctx = document.getElementById('myChart');
      new Chart(ctx, {
        type: 'bar',
        data: {
        labels: ["1999-12-31","2000-01-31","2000-02-29","2000-03-31","2000-04-28"],
          datasets: [{
          label: '# of $',
          data: [111.5, 118.37,102.75,112.25,107.87],
          borderWidth: 1
          }]
          },
            options: {
              scales: {
                y: {
                  beginAtZero: true
          }
        }
      }
    });
  </script>
</body>
</html>
